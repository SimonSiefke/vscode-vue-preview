{
  "[html]": {
    "editor.formatOnSave": false
  },
  "files.exclude": {
    "**/.git": false,
    "**/node_modules": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false
  },
  "search.exclude": {
    "**/node_modules": false,
    "**/bower_components": false,
    "**/dist": false
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true
  },
  "files.trimTrailingWhitespace": false,
  "editor.formatOnSave": true,
  "eslint.autoFixOnSave": true,
  "json.schemas": [
    {
      "fileMatch": [
        "i18n/de.json",
        "i18n/en.json",
        "i18n/es.json",
        "i18n/fr.json",
        "i18n/it.json",
        "i18n/ja.json",
        "i18n/nl.json",
        "i18n/ru.json",
        "i18n/zh-cn.json"
      ],
      "url": "./i18n/_schema.json"
    }
  ],
  "typescript.tsdk": "node_modules/typescript/lib"
}
