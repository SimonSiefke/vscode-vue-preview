{
  "name": "vue-preview",
  "displayName": "Vue Preview",
  "description": "Preview for Vue files",
  "version": "1.0.0",
  "publisher": "SimonSiefke",
  "license": "MIT",
  "homepage": "https://github.com/SimonSiefke/vscode-vue-preview",
  "repository": {
    "type": "git",
    "url": "https://github.com/SimonSiefke/vscode-vue-preview.git"
  },
  "bugs": {
    "url": "https://github.com/SimonSiefke/vscode-vue-preview/issues"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#242b2b",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "vue preview",
    "vue",
    "preview",
    "live"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onCommand:vuePreview.openPreview"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Vue Preview",
      "properties": {
        "vuePreview.autoOpen": {
          "type": "boolean",
          "default": false,
          "description": "Automatically open the preview when a vue file is opened.",
          "scope": "window"
        }
      }
    },
    "commands": [
      {
        "category": "Vue Preview",
        "command": "vuePreview.openPreview",
        "title": "Open Preview"
      }
    ],
    "keybindings": [
      {
        "command": "vuePreview.openPreview",
        "key": "ctrl+alt+p",
        "when": "resourceLangId == vue"
      }
    ],
    "languages": [
      {
        "id": "vue",
        "extensions": [
          ".vue"
        ]
      }
    ]
  },
  "scripts": {
    "dev": "cd ../../ && npm run dev"
  },
  "main": "dist/extensionMain.js",
  "devDependencies": {
    "@types/vscode": "^1.38.0",
    "typescript": "^3.7.0-dev.20191009"
  },
  "dependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "less": "^3.10.3",
    "sass": "^1.23.0",
    "stylus": "^0.54.7",
    "vue-template-compiler": "^2.6.10"
  }
}
